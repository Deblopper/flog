---
layout: nil
---
<?xml version="1.0" encoding="utf-8" ?>
<feed	xmlns="http://www.w3.org/2005/Atom"
		xml:lang="en"
		xml:base="http://blog.debs.io">

	<title>Deb's Personal Weblog</title>
	<link href="http://blog.debs.io"/>
	<link	rel="self"
			type="application/atom+xml"
			href="/atom.xml"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.debs.io/atom.xml</id>
	<author>
		<name>Soumya Deb</name>
	</author>

	{% for post in site.posts %}
	<entry>
		<id>http://blog.debs.io{{ post.url }}</id>
		<link	rel="alternate"
				type="text/html"
				href="http://blog.debs.io{{ post.url }}"/>
		<title>{{ post.title }}</title>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<summary type="text/html">
			{{ post.content | strip_html | truncatewords: 100 }}
			[<a href="http://blog.debs.io{{ post.url }}">Read More</a>]
		</summary>
	</entry>
	{% endfor %}

</feed>
